<odoo>
  <data>

    <record id="tripartite_report_view_tree" model="ir.ui.view">
      <field name="name">tripartite.report.view.tree</field>
      <field name="model">tripartite.report</field>
      <field name="arch" type="xml">
        <tree string="Tripartite report">
          <field name="name" />
          <field name="brokers_ref" />
          <field name="client_ref" />
          <field name="material" />
          <field name="bank" />
          <field name="form_m_to_bank" />
          <field name="form_m_validated_by_bank" />
          <field name="form_m_registered_by_ncs" />
          <field name="delay_impact" />
          <field name="beneficiary" />
          <field name="lc_number" />
          <field name="form_m_number" />
          <field name="curr" />
          <field name="form_m_value" />
          <field name="payment_mode" />
          <field name="comment" />
          <field name="fecd_submitted" />
          <field name="fecd_ack_copy_sent_to_gn" />
          <field name="date_po" />
          <field name="transaction_type" />
          <field name="form_m_application_to_bank" />
          <field name="lc_draft_shared_supplier" />
          <field name="lc_draft_vetted_supplier" />
          <field name="final_telex_received_from_bank" />
          <field name="final_lc_telex" />
        </tree>
      </field>
    </record>

    <record id="tripartite_report_view_form" model="ir.ui.view">
      <field name="name">tripartite.report.view.form</field>
      <field name="model">tripartite.report</field>
      <field name="arch" type="xml">
        <form string="Tripartite report">
          <group>
            <group>
              <field name="name" />
              <field name="brokers_ref" />
              <field name="client_ref" />
              <field name="material" />
              <field name="bank" />
              <field name="form_m_to_bank" />
              <field name="form_m_validated_by_bank" />
              <field name="form_m_registered_by_ncs" />
              <field name="delay_impact" />
              <field name="beneficiary" />
              <field name="lc_number" />
              <field name="form_m_number" />
              <field name="curr" />
              <field name="form_m_value" />
            </group>
            <group>
              <field name="payment_mode" />
              <field name="comment" />
              <field name="fecd_submitted" />
              <field name="fecd_ack_copy_sent_to_gn" />
              <field name="date_po" />
              <field name="transaction_type" />
              <field name="form_m_application_to_bank" />
              <field name="lc_draft_shared_supplier" />
              <field name="lc_draft_vetted_supplier" />
              <field name="final_telex_received_from_bank" />
              <field name="final_lc_telex" />
            </group>
          </group>
        </form>
      </field>
    </record>

    <record id="tripartite_report_action" model="ir.actions.act_window">
      <field name="name">Tripartite Report</field>
      <field name="res_model">tripartite.report</field>
      <field name="view_mode">tree,form</field>
      <field name="domain">[]</field>
      <field name="context">{}</field>
    </record>

    <menuitem id="tripartite_report_menu" name="Tripartite Report" parent="libra_custom_reports_menu" sequence="1"/>

    <menuitem id="tripartite_report_action_menu" name="Tripartite Report" parent="tripartite_report_menu" action="tripartite_report_action" sequence="1"/>


  </data>
</odoo>