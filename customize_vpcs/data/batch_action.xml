<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="in_transit_action" model="ir.actions.server">
            <field name="name">To In Transit</field>
            <field name="model_id" ref="customize_vpcs.model_project_schedule_items"/>
