<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="project_report_wizard_view_form" model="ir.ui.view">
        <field name="name">project.report.wizard.view.form</field>
        <field name="model">project.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Project Report">
                <group>
                    <group>
                        <field name="duty" />
                        <field name="terminal" />
                        <field name="son" />
                        <field name="transportation" />
                    </group>
                    <group>
                        <field name="shipping" />
                        <field name="nfdac" />
                        <field name="agency" />
                        <field name="other" />
                    </group>
                </group>
                <footer>
                     <button name="report_download" class="btn-primary" string="Donwload Excel Report" type="object"/>
                     <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="project_report_wizard_action" model="ir.actions.act_window">
        <field name="name">Project Report</field>
        <field name="res_model">project.report.wizard</field>
        <field name="binding_model_id" ref="project.model_project_project" />
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>