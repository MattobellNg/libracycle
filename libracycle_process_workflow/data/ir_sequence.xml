<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="lbc_ref_number" model="ir.sequence">
            <field name="name">LBC Ref. Number</field>
            <field name="code">lbc.ref.number</field>
            <field name="prefix">LBC%(year)s%(month)s%(day)s</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field eval="False" name="company_id" />
            <field eval="4" name="padding" />
        </record>
        <record id="invoice_number" model="ir.sequence">
            <field name="name">LBC: invoice.number</field>
            <field name="code">lbc.invoice.number</field>
            <field name="prefix">LBC%(year)s%(month)s%(day)s</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field eval="False" name="company_id" />
            <field eval="4" name="padding" />
        </record>
    </data>
</odoo>